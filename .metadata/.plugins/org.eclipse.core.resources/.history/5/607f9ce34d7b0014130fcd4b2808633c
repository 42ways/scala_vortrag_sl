object lists {
  1 to 10

  1 to 10 filter (_ % 2 == 0)

  1 to 10 map (_ * 2)

  (1 to 10) zip (11 to 20)

  List("Peter", "Paul", "Mary") map (_.toUpperCase)

	def fac(n: Int) = (1 to n) reduceLeft(_*_)


	fac(5)

	def squareSum(n: Int) = (1 to n) map(x => x*x ) reduceLeft(_+_)

	squareSum(1)
	squareSum(2)
	squareSum(3)
	squareSum(4)

}